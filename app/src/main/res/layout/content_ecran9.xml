    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/list_delimit"
        android:layout_alignTop="@+id/rel_bar1" />

    <RelativeLayout
        android:id="@+id/rel_bar1"
        style="@style/relative_custom"
        android:layout_below="@+id/textView8"
        android:layout_marginTop="21dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">


        <ro.nubloca.extras.CustomFontTitilliumRegular
            android:id="@+id/textView1"
            style="@style/text_layout_custom"
            android:text="Ajutor" />

        <ImageView
            android:id="@+id/chevron"
            android:layout_width="7dp"
            android:layout_height="30dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_gravity="right"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:background="@drawable/chevron" />

    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_above="@+id/rel_bar2"
        android:background="@color/list_delimit" />

    <RelativeLayout
        android:id="@+id/rel_bar2"
        style="@style/relative_custom"
        android:layout_below="@+id/rel_bar1">


        <ro.nubloca.extras.CustomFontTitilliumRegular
            android:id="@+id/contac"
            style="@style/text_layout_custom"
            android:text="Contact" />

        <ImageView
            android:id="@+id/chevron1"
            android:layout_width="7dp"
            android:layout_height="30dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_gravity="right"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:background="@drawable/chevron" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/rel_bar3"
        android:background="@color/list_delimit" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_above="@+id/rel_bar3"
        android:background="@color/list_delimit" />

    <RelativeLayout
        android:id="@+id/rel_bar3"
        style="@style/relative_custom"
        android:layout_below="@+id/rel_bar2">


        <ro.nubloca.extras.CustomFontTitilliumRegular
            android:id="@+id/textView9"
            style="@style/text_layout_custom"
            android:text="Termeni si Conditii" />

        <ImageView
            android:id="@+id/chevron2"
            android:layout_width="7dp"
            android:layout_height="30dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_gravity="right"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:background="@drawable/chevron" />
    </RelativeLayout>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rel_bar3"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="33dp"
        android:background="@drawable/car_blue" />

    <ro.nubloca.extras.CustomFontTitilliumBold
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView"
        android:layout_centerHorizontal="true"
        android:text="@string/despre_6"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="#3DC6FE"
        android:textStyle="bold" />
</RelativeLayout>
