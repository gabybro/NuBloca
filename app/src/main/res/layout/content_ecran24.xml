<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/btn_white"
    android:layout_marginTop="20dp"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".Ecran24Activity"
    tools:showIn="@layout/activity_ecran24">


    <RelativeLayout
        android:id="@+id/rel1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/border2"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/rel_bar1"
            style="@style/relative_custom"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:clickable="true"
            android:orientation="horizontal">

            <RadioButton
                android:id="@+id/radioButton1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginStart="20dp"
                android:clickable="false" />

            <ro.nubloca.extras.CustomFontTitilliumRegular
                android:id="@+id/text1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginStart="20dp"
                android:paddingBottom="10dp"
                android:paddingTop="10dp"
                android:text="@string/istoric_numere_1"
                android:textColor="@color/text_layout_custom"
                android:textSize="23sp" />


        </LinearLayout>

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rel2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/border2"
        android:layout_below="@+id/rel1"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/rel_bar2"
            style="@style/relative_custom"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:clickable="true"
            android:orientation="horizontal">

            <RadioButton
                android:id="@+id/radioButton2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginStart="20dp"
                android:clickable="false" />

            <ro.nubloca.extras.CustomFontTitilliumRegular
                android:id="@+id/text2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginStart="20dp"
                android:paddingBottom="10dp"
                android:paddingTop="10dp"
                android:text="@string/istoric_numere_2"
                android:textColor="@color/text_layout_custom"
                android:textSize="23sp" />


        </LinearLayout>

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rel3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/border2"
        android:layout_below="@+id/rel2"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/rel_bar3"
            style="@style/relative_custom"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:clickable="true"
            android:orientation="horizontal">

            <RadioButton
                android:id="@+id/radioButton3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginStart="20dp"
                android:clickable="false" />

            <ro.nubloca.extras.CustomFontTitilliumRegular
                android:id="@+id/text3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginStart="20dp"
                android:paddingBottom="10dp"
                android:paddingTop="10dp"
                android:text="@string/istoric_numere_3"
                android:textColor="@color/text_layout_custom"
                android:textSize="23sp" />


        </LinearLayout>

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rel4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/border2"
        android:layout_below="@+id/rel3"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/rel_bar4"
            style="@style/relative_custom"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:clickable="true"
            android:orientation="horizontal">

            <RadioButton
                android:id="@+id/radioButton4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginStart="20dp"
                android:clickable="false" />

            <ro.nubloca.extras.CustomFontTitilliumRegular
                android:id="@+id/text4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginStart="20dp"
                android:paddingBottom="10dp"
                android:paddingTop="10dp"
                android:text="@string/istoric_numere_4"
                android:textColor="@color/text_layout_custom"
                android:textSize="23sp" />


        </LinearLayout>

    </RelativeLayout>
</RelativeLayout>
