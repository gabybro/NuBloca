<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".Ecran11Activity"
    tools:showIn="@layout/activity_ecran11">

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignParentTop="true"
        android:layout_above="@+id/rel_bar1"
        android:background="@color/list_delimit"
        android:layout_marginTop="11dp" />

    <RelativeLayout
        android:id="@+id/rel_bar1"
        style="@style/relative_custom">


        <ro.nubloca.extras.CustomFontTitilliumRegular
            android:id="@+id/textView1"
            style="@style/text_layout_custom"
            android:text="@string/ajutor_1" />

        <ImageView
            android:id="@+id/chevron"
            android:layout_width="7dp"
            android:layout_height="30dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_gravity="right"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:background="@drawable/chevron" />

    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_above="@+id/rel_bar2"
        android:background="@color/list_delimit" />

    <RelativeLayout
        android:id="@+id/rel_bar2"
        style="@style/relative_custom"
        android:layout_below="@+id/rel_bar1">


        <ro.nubloca.extras.CustomFontTitilliumRegular
            android:id="@+id/contac"
            style="@style/text_layout_custom"
            android:text="@string/ajutor_2" />

        <ImageView
            android:id="@+id/chevron1"
            android:layout_width="7dp"
            android:layout_height="30dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_gravity="right"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:background="@drawable/chevron" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/rel_bar3"
        android:background="@color/list_delimit" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_above="@+id/rel_bar3"
        android:background="@color/list_delimit" />

    <RelativeLayout
        android:id="@+id/rel_bar3"
        style="@style/relative_custom"
        android:layout_below="@+id/rel_bar2">


        <ro.nubloca.extras.CustomFontTitilliumRegular
            android:id="@+id/textView9"
            style="@style/text_layout_custom"
            android:text="@string/ajutor_3" />

        <ImageView
            android:id="@+id/chevron2"
            android:layout_width="7dp"
            android:layout_height="30dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_gravity="right"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:background="@drawable/chevron" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rel_bar4"
        style="@style/relative_custom"
        android:layout_below="@+id/rel_bar3">


        <ro.nubloca.extras.CustomFontTitilliumRegular
            android:id="@+id/textView10"
            style="@style/text_layout_custom"
            android:text="@string/ajutor_4" />

        <ImageView
            android:id="@+id/chevron3"
            android:layout_width="7dp"
            android:layout_height="30dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_gravity="right"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:background="@drawable/chevron" />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/rel_bar4"
        android:background="@color/list_delimit" />


</RelativeLayout>
