<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/btn_white"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".Ecran20Activity"
    tools:showIn="@layout/activity_ecran20">


    <ro.nubloca.extras.CustomFontTitilliumRegular
        android:id="@+id/catreCeNumarId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:paddingBottom="@dimen/marginBottom_catreCeNumarId"
        android:paddingLeft="@dimen/marginLeft_catreCeNumarId"
        android:paddingStart="@dimen/marginLeft_catreCeNumarId"
        android:paddingTop="@dimen/marginTop_catreCeNumarId"
        android:text="@string/contact_1"
        android:textSize="@dimen/textSize_catreCeNumarId" />

    <LinearLayout
        android:id="@+id/linear_plate_holder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/catreCeNumarId"
        android:layout_centerInParent="true"
        android:descendantFocusability="beforeDescendants"
        android:focusableInTouchMode="true"
        android:gravity="center_horizontal"
        android:orientation="horizontal">


    </LinearLayout>

    <LinearLayout
        android:id="@+id/alegeTipInmatriculare"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linear_plate_holder"
        android:layout_marginBottom="@dimen/paddingBottom_NumeTipInmatriculare"
        android:layout_marginTop="@dimen/marginTop_alegeTipInmatriculare"
        android:orientation="vertical"
        android:paddingLeft="@dimen/marginLeft_catreCeNumarId"
        android:paddingRight="@dimen/marginLeft_catreCeNumarId"
        android:paddingStart="@dimen/marginLeft_catreCeNumarId">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical|start"
            android:orientation="horizontal">

            <ro.nubloca.extras.CustomFontTitilliumRegular
                android:id="@+id/text_TipInmatriculare"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/contact_2"
                android:textSize="@dimen/textSize_TipInmatriculare" />

            <ro.nubloca.extras.CustomFontTitilliumBold
                android:id="@+id/nume_tip_inmatriculare"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/paddingLeftRight_NumeTipInmatriculare"
                android:layout_marginStart="@dimen/paddingLeftRight_NumeTipInmatriculare"
                android:ellipsize="marquee"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:lines="1"
                android:marqueeRepeatLimit="marquee_forever"
                android:maxWidth="@dimen/maxWidth_TipInmatriculare"
                android:scrollHorizontally="true"
                android:singleLine="true"
                android:text="@string/contact_3"
                android:textSize="@dimen/textSize_tipInmatriculare" />

            <ImageView
                android:id="@+id/flag1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/paddingLeft_Flag"
                android:layout_marginStart="@dimen/paddingLeft_Flag" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lin1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start|center_vertical"
            android:orientation="horizontal">


            <TextView
                android:id="@+id/alegeAltTip"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/contact_4"
                android:textColor="@color/contact_4_color"
                android:textSize="@dimen/textSize_tipInmatriculare" />

            <ImageView
                android:id="@+id/chevron_orange"
                android:layout_width="10dp"
                android:layout_height="14dp"
                android:layout_marginLeft="@dimen/marginLeft_ChevronOrange"
                android:layout_marginStart="@dimen/marginLeft_ChevronOrange"
                android:background="@drawable/chevron_orange_right"
                android:clickable="true" />
        </LinearLayout>
    </LinearLayout>


    <LinearLayout
        android:id="@+id/textSalutAiParcat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/alegeTipInmatriculare"
        android:background="@drawable/border3"
        android:clickable="true"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/heightLayout_salutAiParcat"
            android:background="@drawable/list_ripple_effect"
            android:gravity="center_vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/paddingLeftRight_bar"
                android:layout_marginLeft="@dimen/marginLeft_catreCeNumarId"
                android:layout_marginRight="@dimen/paddingLeftRight_bar"
                android:layout_marginStart="@dimen/marginLeft_catreCeNumarId"
                android:gravity="start|center_vertical"
                android:orientation="horizontal">

                <ro.nubloca.extras.CustomFontTitilliumRegular
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:text="@string/contact_5"
                    android:textColor="@color/black"
                    android:textSize="@dimen/textSize_salutAiParcat" />


            </LinearLayout>

            <LinearLayout
                android:id="@+id/linear1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="end|center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView11"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/height_bar"
                    android:layout_marginLeft="@dimen/paddingLeftRight_bar"
                    android:layout_marginRight="@dimen/paddingLeftRight_bar"
                    android:background="@drawable/bar"
                    android:gravity="center_vertical" />


                <ImageView
                    android:id="@+id/chevron1"
                    android:layout_width="7dp"
                    android:layout_height="@dimen/height_bar"
                    android:layout_marginLeft="@dimen/paddingLeftRight_bar"
                    android:layout_marginRight="@dimen/paddingLeftRight_bar"
                    android:background="@drawable/chevron"
                    android:gravity="center_vertical" />

            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentBottom="true"
        android:layout_below="@+id/textSalutAiParcat"
        android:layout_marginTop="1dp"
        android:background="@drawable/blue_car1">

        <LinearLayout
            android:id="@+id/cumFunctioneaza"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:paddingLeft="@dimen/paddingLeftRight_Cumfunctioneaza"
            android:paddingStart="@dimen/paddingLeftRight_Cumfunctioneaza"
            android:paddingRight="@dimen/paddingLeftRight_Cumfunctioneaza"
            android:paddingEnd="@dimen/paddingLeftRight_Cumfunctioneaza"
            android:clickable="true">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/marginTop_cumfunctioneazaEcran20"
                android:paddingBottom="@dimen/marginTop_cumfunctioneazaEcran20"
                android:text="@string/contact_6"
                android:textColor="@color/contact_67_color"
                android:textSize="@dimen/textSize_NumeTipInmatriculare" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/istoricNumere"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:paddingLeft="@dimen/paddingLeftRight_Cumfunctioneaza"
            android:paddingStart="@dimen/paddingLeftRight_Cumfunctioneaza"
            android:paddingRight="@dimen/paddingLeftRight_Cumfunctioneaza"
            android:paddingEnd="@dimen/paddingLeftRight_Cumfunctioneaza"
            android:clickable="true">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/marginTop_cumfunctioneazaEcran20"
                android:paddingBottom="@dimen/marginTop_cumfunctioneazaEcran20"
                android:text="@string/contact_7"
                android:textColor="@color/contact_67_color"
                android:textSize="@dimen/textSize_NumeTipInmatriculare" />
        </LinearLayout>

    </RelativeLayout>
</RelativeLayout>